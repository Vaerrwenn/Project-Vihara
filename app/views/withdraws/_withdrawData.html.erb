<div class="row">
        <strong>Data Penarikan:</strong>
    </div>
    <div class="row">
        <%= link_to new_member_withdraw_path(@member), class: "btn btn-sm btn-primary", data: { toggle: "modal", target: "#newDraw" } do %>
            <i class="fas fa-plus"></i> Penarikan
        <% end %>
    </div>
    <div class="row">
        <table class="table table-sm table-custom table-bordered" id="withdrawTable">
            <thead class="thead">
                <tr>
                    <th scope="col" style="width: 15%">Tanggal</th>
                    <th scope="col" style="width: 15%">No. Kwitansi</th>
                    <th scope="col" style="width: 15%">Jenis</th>
                    <th scope="col" style="width: 20%">Jumlah</th>
                    <th scope="col" style="width: 40%">Deskripsi</th>
                    <th scope="col" style="width: 10%">Hapus</th>
                </tr>
            </thead>
            <tbody>
                <% if @member.withdraws.exists? %>
                    <%= render 'withdraws/withdrawal' %>
                <% else %>
                    <tr>
                        <td colspan="6">Tidak ada data.</td>
                    </tr>
                <% end %>
            </tbody>
        </table>
    </div>